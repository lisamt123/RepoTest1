                    function stripOnloadActionParam(reloadUrl) {
                        if( !getParam().onloadAction &&
                            !getParam().onloadaction ) {
                            return reloadUrl;   
                        }
                        
                        var convertedUrl = reloadUrl.replace(/&onloadAction=send/gi, '');
