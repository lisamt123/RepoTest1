                                        <apex:outputLabel value="{!$ObjectType.icBase__Development_Requirement__c.fields.icBase__Description__c.label}" />
                                        <apex:inputTextarea value="{!developmentDescription}" cols="75" rows="15" richText="false"/>
                                    </apex:pageBlockSectionItem>
                                    <apex:pageBlockSectionItem >
                                        <apex:outputLabel value="{!$ObjectType.icBase__Development_Requirement__c.fields.icBase__Status__c.label}" />
                                        <apex:selectList value="{!selectedDevelopmentStatus}" size="1">
                                            <apex:selectOptions value="{!developmentStatus}" />
                                        </apex:selectList>
                                    </apex:pageBlockSectionItem>                        
                                </apex:pageBlockSection>
                            </apex:pageBlock>

                            <!-- GENERIC REQUIREMENT OVERLAY START -->
                            <apex:pageBlock id="pbGenericEdit" rendered="{!showGenericEdit}" title="{!$ObjectType.icBase__Generic_Requirement__c.Label}" mode="edit" helpTitle="{!$Label.icBase__Get_help}" helpUrl="http://www.implementationcloud.com/WebpageUserGuide" >
                                <apex:pageBlockButtons location="top">
                                    <apex:commandButton value="{!$Label.site.save}" styleClass="btn" action="{!SaveGenericRequirement}" immediate="false"/>
                                    <apex:commandButton value="{!$Label.site.cancel}" onclick="hideOverlay()" styleClass="btn" action="{!hideDialog}" reRender="genricBlock, overlayFloater, pnlRunCode" immediate="true" />
                                </apex:pageBlockButtons>
                                <script>
                                    if({!showGenericEdit} == true){
                                        $("#divWrapper").attr('style','display:block');
                                    }
                                </script>
                                <apex:pageMessages />
                                <apex:pageBlockSection columns="1" collapsible="false" rendered="true">
                                    <apex:pageBlockSectionItem >
                                        <apex:outputLabel value="{!$ObjectType.icBase__Generic_Requirement__c.fields.Name.label}" />
                                        <apex:panelGroup >
                                            <apex:inputText value="{!genericRequirementName}" style="width:250px;"/>
                                            <apex:outputLabel value="{!$Label.icBase__Required_Label}" style="color:red;" />
                                        </apex:panelGroup>
                                    </apex:pageBlockSectionItem>
                                    <apex:pageBlockSectionItem >
                                        <apex:outputLabel value="{!$ObjectType.icBase__Generic_Requirement__c.fields.icBase__Type__c.label}" />
                                        <apex:selectList value="{!selectedGenericType}" required="true" size="1">
                                            <apex:selectOptions value="{!genericRequirementType}" />
                                        </apex:selectList>
                                    </apex:pageBlockSectionItem>
                                </apex:pageBlockSection>
                                <br/>
                                <apex:pageBlockSection columns="1" collapsible="false" rendered="true"> 
                                    <apex:pageBlockSectionItem >
                                        <apex:outputLabel value="{!$ObjectType.icBase__Generic_Requirement__c.fields.icBase__Description__c.label}" />
                                        <apex:inputTextarea value="{!genericRequirementDescription}" cols="75" rows="15" richText="false"/>
                                    </apex:pageBlockSectionItem>
                                    <apex:pageBlockSectionItem >
                                        <apex:outputLabel value="{!$ObjectType.icBase__Generic_Requirement__c.fields.icBase__Status__c.label}" />
                                        <apex:selectList value="{!selectedGenericStatus}" size="1">
                                            <apex:selectOptions value="{!genericRequirementStatus}" />
                                        </apex:selectList>
                                    </apex:pageBlockSectionItem>
                                </apex:pageBlockSection>
                            </apex:pageBlock>
                        </div>
                    </apex:outputPanel>
                </apex:facet>
            </apex:actionStatus>
        </div>
        <apex:outputPanel id="pnlRunCode" rendered="{!OR(showGenericEdit, showDevelopmentEdit, showObjectEdit, showFieldEdit)}">
            <script>
                initPnlRunCode();
                repositionFloater();
                $("#overlay").attr('class','overlayBackgroundVisible');
            </script>
        </apex:outputPanel>
        <apex:actionStatus id="pleaseWaitStatus" onstart="pleaseWaitStart()" onstop="pleaseWaitStop()" />
        <script>
            $(document).ready(function(){
                if(document.getElementById('overlay') == undefined){
                    $('<div id="overlay" class="overlay-mask" style="display:none; position:fixed; top:0px; bottom:0px; left:0px; right:0px; z-index:100;"> </div>').appendTo($('body'));
                }
                if(document.getElementById('overlayPanel') == undefined){
                    $("<div id='overlayPanel'><div id='plsWait'><br /><img src='/img/loading32.gif' /><br /><br /><h1>{!$Label.icBase__Please_wait}</h1></div></div><div id='overlay' class='' style='display:none; position:fixed; top:0px; bottom:0px; left:0px; right:0px; z-index:100;'> </div>").appendTo($('body'));
                    $("#plsWait").attr('class','white_content');
                    $("#plsWait").attr('style','display:none');
                    $("#overlay").attr('class','');
                }                
            });
        </script>
        
        <apex:includeScript value="{!URLFOR($Resource.icBase__Library, 'scripts/jqueryMin.js')}"/>
        <apex:includeScript value="{!URLFOR($Resource.icBase__Library, 'scripts/jqueryUI.js')}"/>
        <apex:includeScript value="{!URLFOR($Resource.icBase__Library, 'scripts/DefinitionsPageJS.js')}"/>
        <apex:stylesheet value="{!URLFOR($Resource.icBase__Library, 'styles/overlayCSS.css')}"/>
    </apex:form>    
</apex:page>