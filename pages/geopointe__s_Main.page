                <apex:column headerValue="Description" value="{!ap.Description}"/>
                <apex:column headerValue="Batch Assign">
                    <apex:commandLink value="Run Assignment Plan" action="{!runAssignmentPlan}" rendered="{! ap.Status = 'Active' }">
