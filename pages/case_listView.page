<apex:page standardController="Case" extensions="case_ctrl_listView">
<apex:form >
     
    <apex:stylesheet value="{!URLFOR($Resource.bootstrap, 'bootstrap.css')}" />
    <apex:stylesheet value="{!URLFOR($Resource.sharedcss, 'sharedcss.css')}" />
    
    
    <style>
		
		.hoverLink {  

		    margin:0; 
		    padding:0; 
		    display:inline-block; 
		    cursor:default; 
		    z-index:9999;
		}
		
		.hoverLink .hoverLinkContent { 
		
		    padding:0;  
		    display: none; 
		    position: absolute; 
		    left: 0;
		    z-index: 5000; 
		    white-space: nowrap;
		    margin-top: -2px;
		}
		
		body .pbBody table.list tr.dataRow th.footerColStyle {
		
		    border-width: 0;
		}
		
		body .pbBody table.list tr.dataRow th.footerStyle {
		
		    border-width: 0;
		}
		
		.totalRow .footerColStyle {
		
		    color:#666;
		    text-align:center;
		    background-color:#F1F1F1;
		    padding:5px 2px 5px 5px;
		}
		
		.totalRow .footerStyle {
		
		    text-align:right;
		    background-color:#FFF;
		    padding:5px 2px 5px 5px;
		}
		
		.headerRow .customHeaderClass th {
		
		    border-left-width: 0;
		    background: #FFF;
		    border-width: 0 0 1px 1px;
		    color: black;
		    font-size: .9em;
		    font-weight: bold;
		    padding: 5px 2px 4px 5px;
		    border: 1px solid #D4DADC;
		    border-bottom: 2px solid #CCC;
		    white-space: nowrap;
		    border-color: #ECECEC;
		    border-right: 1px solid #D4DADC;
		    vertical-align: inherit;
		    border-collapse: separate;
		    border-spacing: 2px 2px;
		}
		
		th.currencyAlign {
		
		    text-align:right;
		}
		
		body .pbBody table.list tr.dataRow th {
		
		    border-width: 0 0 1px 1px;
		}
		
		body .pbBody table.list tr.last > th, body .pbBody table.list tr.last > td {
		
		    border-bottom-width:1px;
		}
	</style>
    
    
    
    
    
    
    
    <style>
        
        .highlight {
        	
        	background-color:#E3F3FF;
        }
        
        
        .userName {
            font-weight: bold;
            color: #015ba7;
            text-decoration: none;
        }
        
        .feeditemactionlink {
            color: #015ba7;
        }
        
        .displayDate {
            line-height: 13px;
            font-weight: normal;
            color: #7d7d84;
            text-decoration: none;
        }
        
        .feeditem {
            min-height: 47px;
            white-space: normal;
            word-wrap: break-word;
            margin: 0 10px 0px 15px;
            padding: 8px 4px 8px 0;
            border-top: 1px solid #fff;
            border-bottom: 1px solid #e6e6e6;
            display:block;
            line-height: 15px;
        }
        
        .feedFilter {
            min-height: 20px;
            white-space: normal; 
            word-wrap: break-word;
            margin: 0 10px 8px 15px;
            padding: 8px 4px 8px 0;
            border-top: 1px solid #e6e6e6;
            border-bottom: 1px solid #fff;
            display:block;
            line-height: 15px;
        }
        
        .feedOverflowWrapper {
            height: 380px;
            overflow: auto;
            display: block;
        }
        
        .feeditemcontent {
            display: block;
            margin-left: 0px;
            border-bottom: none;
        }
        
        .feeditembodyandfooter {
            position: relative;
            display: block;
        }
        
        .feeditembody {
            /*
            padding-right: 40px;
            */
            display: block;
        }
        
        .feeditemfooter {
            display: block;
            font-size: .92em;
            line-height: 13px;
            margin: 4px 0;
        }
        
        .preamblecontainer {
            display: inline;
            padding-bottom: 4px;
            padding-right: 5px;
        }
        
        .feedWrapper {
            border: 1px solid #e1e0e0;
            border-top-color: #d1d1d1;
            -webkit-border-radius: 5px;
            border-radius: 5px;
        }
        
        .innerFeedWrapper { 
            /*
            width: 546px;
            */
            width: 90%; 
        }
        
        .row-fluid .span4 {
            width: 100% !important;
        }
        
        .sortLink {
            color: #015ba7;
            font-size: .9em;
        }
        
        .sortText {
            font-size: .9em;
        }
        
        .row-fluid .span4 {
            width: 40%;
        }
        
        body {
            overflow-y: visible !important;
            overflow-x: visible !important;
        }
        
        .showMoreSection {
            float:left;
            padding: 30px 0 10px 0;
            margin-left: 15px;
        }
        
        .clearDiv {
            clear:both;
        }
        
        .emptyListDiv {
            margin-left: 15px;
        }
        
        .plusIcon {
            vertical-align: middle;
            display: inline;
            width: auto;
            background-image: url(/img/feeds/follow_sprite.png);
            background-repeat: no-repeat;
            background-position: 0 -27px;
            background-color: #fff;
            height: 15px;
            padding-bottom: 3px;
            border: 0;
            margin-right: 3px;
        }
        
    </style>
    
    
    
    
    
    <br />
    
