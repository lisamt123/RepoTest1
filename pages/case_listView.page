            <apex:outputLink target="_parent" value="/a1A/e?saveURL=%2F{!Case.Id}&retURL=%2F{!Case.Id}&CF00N70000002NOt2={!Case.CaseNumber}&CF00N70000002NOt2_lkid={!Case.Id}&RecordType=012700000005eL1&00N70000002NOtM=assigned&CF00N70000002NOth={!$User.FirstName}+{!$User.LastName}" style="font-weight:bold;">Assign</apex:outputLink>
