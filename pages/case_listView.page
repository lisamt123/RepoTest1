                            <b>List is empty. Select filters above to populate list.</b>
                        </div>
                    </apex:outputPanel>
                    </div>
                    <div class="showMoreSection">
                        
                        <apex:commandLink styleClass="sortLink" value="{!scrollLinkText}" action="{!showMoreLess}" status="status2" rerender="mainOP" />
                        
                        <!-- 
                        <apex:commandLink styleClass="sortLink" value="{!showMoreLabel} ({!totalRecords})" action="{!showMoreLess}" status="status2" rerender="mainOP" />
                         -->
                        
                        <!--  
                        <a href="#" title="Cannot view this this record." class="feeditemactionlink cxaddcommentaction">Show More ({!totalRecords})</a>
                         -->
                    </div>
                    <div class="clearDiv">
                    </div>
                </div>
            </div>
        </apex:outputPanel>
    </div>
    
     
    <script type="text/javascript">
        function switchActionItemVisibility(objId) {
            if(document.getElementById('action_link_' + objId).innerHTML == 'View') {  
                
                document.getElementById('action_link_' + objId).innerHTML = 'Hide';   
                document.getElementById('action_options_' + objId).style.display = '';
               // document.getElementById('action_block_' + objId).title = 'Collapse'; 
            } 
            else {  
                document.getElementById('action_link_' + objId).innerHTML = 'View';   
                document.getElementById('action_options_' + objId).style.display = 'none';
                //document.getElementById('action_block_' + objId).title = 'Expand';   
            }
        }
    </script>
     
