            <apex:outputLink target="_parent" value="/a1A/e?saveURL=%2F{!Case.Id}&CF00N70000002NOt2={!Case.CaseNumber}&CF00N70000002NOt2_lkid={!Case.Id}&retURL=%2F{!Case.Id}&RecordType=012700000005eLB&00N70000002NOtM=additional information needed" style="font-weight:bold;">Request More Info</apex:outputLink>
            <apex:outputLink target="_parent" value="/a1A/e?saveURL=%2F{!Case.Id}&CF00N70000002NOt2={!Case.CaseNumber}&CF00N70000002NOt2_lkid={!Case.Id}&retURL=%2F{!Case.Id}&RecordType=012700000005eLG&CF00N70000002NOth={!Case.Assigned_To__c}&00N70000002NOtM=submitting additional information" style="font-weight:bold;">Submit More Info</apex:outputLink>
