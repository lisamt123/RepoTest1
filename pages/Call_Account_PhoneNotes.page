<apex:page standardController="account" tabStyle="account"
	id="ContactCallNotes" name="ContactCallNotes" showHeader="false">
	<body onload="foo();">
	</body>
	<script type="text/javascript">
function foo(){
if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)){ //test for MSIE x.x;
   window.document.write("{!account.phone}");
}
else{
 var ifrm = document.createElement("IFRAME"); 
 ifrm.setAttribute("src", "http://192.168.2.134:8080/test/Test.html?pn={!account.phone}&ext={!$User.Extension}&sid={!$Api.Session_ID}&whatId={!Account.Id}"); 
 ifrm.setAttribute("frameBorder","0");
 window.document.body.appendChild(ifrm); 
}
}
</script>
</apex:page>