    <decisions>
        <name>Route_for_Bonus_Gifts</name>
        <label>Route for Bonus Gifts</label>
        <locationX>361</locationX>
        <locationY>142</locationY>
        <defaultConnector>
            <targetReference>Thank_you_Gifts_Basic_Membership</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Basic Membership Level</defaultConnectorLabel>
        <rules>
            <name>Monthly_Membership_Route</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>vaPledge</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>144.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Thank_you_Gifts_Monthly_Membership</targetReference>
            </connector>
            <label>Monthly Membership Route</label>
        </rules>
        <rules>
            <name>Leadership_Level_Route</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>vaPledge</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>365.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Thank_you_Gifts_Leadership_Membership</targetReference>
            </connector>
            <label>Leadership Level Route</label>
        </rules>
    </decisions>
