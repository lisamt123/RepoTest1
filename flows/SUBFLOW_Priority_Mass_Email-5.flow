    <decisions>
        <name>Evaluate_Passed_Setting</name>
        <label>Evaluate Passed Setting</label>
        <locationX>593</locationX>
        <locationY>160</locationY>
        <defaultConnectorLabel>Error</defaultConnectorLabel>
        <rules>
            <name>PriorityPartial</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varMassChatterSetting</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>PriorityPartial</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Email_Body_for_Priority</targetReference>
            </connector>
            <label>Priority - Partial</label>
        </rules>
        <rules>
            <name>PriorityFull</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varMassChatterSetting</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>PriorityFull</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Chatter_Body_for_Force</targetReference>
            </connector>
            <label>Priority - Full</label>
        </rules>
    </decisions>
    <description>Kandy Richert replaces Brian Fullmer. still need full edit.</description>
    <label>SUBFLOW - Priority Mass Email</label>
