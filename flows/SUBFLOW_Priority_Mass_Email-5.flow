    <assignments>
        <name>Chatter_Body_for_Force</name>
        <label>Chatter Details for Force</label>
        <locationX>472</locationX>
        <locationY>276</locationY>
        <assignmentItems>
            <assignToReference>varChatterBody</assignToReference>
            <operator>Assign</operator>
            <value>
