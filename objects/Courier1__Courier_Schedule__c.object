    <validationRules>
        <fullName>Courier1__Validate_Dont_Run_On_Weekends</fullName>
        <active>true</active>
        <errorConditionFormula>if(
AND(
IsPickVal( Courier1__Trigger_Interval__c , &quot;Weekly&quot;) ,

Courier1__Don_t_Run_on_Weekends__c,

OR(
IsPickVal( Courier1__Trigger_Run_Day__c, &quot;Saturday&quot;),
IsPickVal( Courier1__Trigger_Run_Day__c, &quot;Sunday&quot;)
)
),
true, false)</errorConditionFormula>
        <errorDisplayField>Courier1__Don_t_Run_on_Weekends__c</errorDisplayField>
        <errorMessage>This setting won&apos;t work with your current interval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Courier1__Validate_Hour_to_Run_On</fullName>
        <active>true</active>
        <errorConditionFormula>IF( AND(

OR( ISPICKVAL( Courier1__Trigger_Interval__c , &quot;Daily&quot;),
ISPICKVAL( Courier1__Trigger_Interval__c , &quot;Weekly&quot;),
ISPICKVAL( Courier1__Trigger_Interval__c, &quot;Monthly&quot;) ),

ISPICKVAL( Courier1__Trigger_Run_Time__c, &quot;&quot; )),
True , False )</errorConditionFormula>
        <errorDisplayField>Courier1__Trigger_Run_Time__c</errorDisplayField>
        <errorMessage>For the interval chosen, you must also choose a Hour to Run On.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Courier1__Validate_Monthly_DOW</fullName>
        <active>true</active>
        <errorConditionFormula>IF( AND(

ISPICKVAL( Courier1__Trigger_Interval__c, &quot;Monthly&quot;) ,

OR(
  ISPICKVAL( Courier1__Trigger_Run_Day__c, &quot;the 1st ...&quot;) ,
  ISPICKVAL( Courier1__Trigger_Run_Day__c, &quot;the 2nd ...&quot;),
  ISPICKVAL( Courier1__Trigger_Run_Day__c, &quot;the 3rd ...&quot;),
  ISPICKVAL( Courier1__Trigger_Run_Day__c, &quot;the 4th ...&quot;),
  ISPICKVAL( Courier1__Trigger_Run_Day__c, &quot;the last ...&quot;)) ,

ISPICKVAL(  Courier1__Monthly_Day_of_the_Week__c , &quot;&quot; )),
True , False )</errorConditionFormula>
        <errorDisplayField>Courier1__Monthly_Day_of_the_Week__c</errorDisplayField>
        <errorMessage>For the interval chosen, you must also choose a Monthly Day of the Week.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Courier1__Validate_Monthly_DOW_2</fullName>
        <active>true</active>
        <errorConditionFormula>IF( AND(

ISPICKVAL( Courier1__Trigger_Interval__c, &quot;Monthly&quot;) ,

!OR(
  ISPICKVAL( Courier1__Trigger_Run_Day__c, &quot;the 1st ...&quot;) ,
  ISPICKVAL( Courier1__Trigger_Run_Day__c, &quot;the 2nd ...&quot;),
  ISPICKVAL( Courier1__Trigger_Run_Day__c, &quot;the 3rd ...&quot;),
  ISPICKVAL( Courier1__Trigger_Run_Day__c, &quot;the 4th ...&quot;),
  ISPICKVAL( Courier1__Trigger_Run_Day__c, &quot;the last ...&quot;)) ,

!ISPICKVAL(  Courier1__Monthly_Day_of_the_Week__c , &quot;&quot; )),
True , False )</errorConditionFormula>
        <errorDisplayField>Courier1__Monthly_Day_of_the_Week__c</errorDisplayField>
        <errorMessage>For the interval chosen, you may not choose a Monthly Day of the Week.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Courier1__Validate_Next_Run_Date</fullName>
        <active>true</active>
        <description>Ensures that the &quot;minutes&quot; portion is the top of an hour, ie, &quot;:00&quot;</description>
        <errorConditionFormula>IF
(
AND
(
TEXT( Courier1__Next_Run_Date__c ) &lt;&gt; &quot;&quot;,

MID( TEXT(Courier1__Next_Run_Date__c), 14, 3) &lt;&gt; &quot;:00&quot;
) ,
true, false
)</errorConditionFormula>
        <errorDisplayField>Courier1__Next_Run_Date__c</errorDisplayField>
        <errorMessage>The &quot;minutes&quot; part must be &quot;:00 AM&quot; or &quot;:00 PM&quot;. For example, &quot;10:00 AM&quot; is valid, but &quot;10:07 AM&quot; is not.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Courier1__Validate_Run_Every_X_Hours</fullName>
        <active>true</active>
        <errorConditionFormula>IF( AND(

ISPICKVAL( Courier1__Trigger_Interval__c , &quot;More than once a day&quot;),

ISPICKVAL( Courier1__Trigger_Hourly_Interval__c, &quot;&quot; )),
True , False )</errorConditionFormula>
        <errorDisplayField>Courier1__Trigger_Hourly_Interval__c</errorDisplayField>
        <errorMessage>For the interval chosen, you must also choose a value in this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Courier1__Validate_Run_On_Day</fullName>
        <active>true</active>
        <errorConditionFormula>IF( AND(

OR( ISPICKVAL( Courier1__Trigger_Interval__c , &quot;Weekly&quot;),
ISPICKVAL( Courier1__Trigger_Interval__c, &quot;Monthly&quot;) ),

ISPICKVAL( Courier1__Trigger_Run_Day__c, &quot;&quot; )),
True , False )</errorConditionFormula>
        <errorDisplayField>Courier1__Trigger_Run_Day__c</errorDisplayField>
        <errorMessage>For the interval chosen, you must also choose a Day to Run On.</errorMessage>
    </validationRules>
    <validationRules>
