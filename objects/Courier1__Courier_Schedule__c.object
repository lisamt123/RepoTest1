        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var id = &quot;{!Courier1__Courier_Schedule__c.Courier1__Report_Id__c}&quot;;
var reportParameter = &quot;{!Courier1__Courier_Schedule__c.Courier1__Report_Parameter__c}&quot;;
if( id != &quot;&quot; )
{
var api_server = &quot;{!API.Partner_Server_URL_80}&quot;;
var slash = api_server.indexOf(&quot;/&quot;, 9);
var server = api_server.substr(0, slash);

var param = &quot;&quot;;
if( reportParameter.length &gt; 0 )
{
  reportParameter = reportParameter.replace(&quot;?&quot;, &quot;&quot;);
   var paramPrefix = reportParameter.substring(2,0).toLowerCase();
   if( paramPrefix != &quot;pv&quot;)
      param = &quot;?pv0=&quot; + reportParameter ;
   else
      param = &quot;?&quot; + reportParameter ;
} 

var result=window.open(server + &quot;/&quot; + id + param, &quot;Report&quot;);
}</url>
