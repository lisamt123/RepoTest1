IsPickVal( Courier1__Start_Hour__c , &quot;09:00 AM&quot;),
IsPickVal( Courier1__Start_Hour__c , &quot;10:00 AM&quot;),
IsPickVal( Courier1__Start_Hour__c , &quot;11:00 AM&quot;),
IsPickVal( Courier1__Start_Hour__c , &quot;12:00 PM&quot;),
IsPickVal( Courier1__Start_Hour__c , &quot;01:00 PM&quot;),
IsPickVal( Courier1__Start_Hour__c , &quot;02:00 PM&quot;),
IsPickVal( Courier1__Start_Hour__c , &quot;03:00 PM&quot;),
IsPickVal( Courier1__Start_Hour__c , &quot;04:00 PM&quot;),
