    <webLinks>
        <fullName>Courier1__Disable_Schedules</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Disable Schedule(s)</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>// Enable Ajax Toolkit
{!REQUIRESCRIPT(&quot;/soap/ajax/8.0/connection.js&quot;)} 

// Get the Ids of the selected records
var records = {!GETRECORDIDS( $ObjectType.Courier1__Courier_Schedule__c )};
var schedRecords = [];

// Update the selected records
if( records[0] == null )
{
    alert(&quot;Please select at least one row&quot;);
}
else
{
    for ( var n in records ) {
        var c = new sforce.SObject(&quot;Courier1__Courier_Schedule__c&quot;);
        c.id = records[n];
        c.Courier1__Enabled__c = false;
        schedRecords.push(c);
    }
}

// Perform the update
result = sforce.connection.update(schedRecords);

// Reload the window
window.location.reload();</url>
    </webLinks>
