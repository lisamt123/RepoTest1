    <validationRules>
        <fullName>Cannot_Modify_Approval_Status</fullName>
        <active>true</active>
        <description>Cannot edit the record if the Approval Status is not Not Submitted and you are not the Approving Manager, PTO Approver, SF Admin, or HR staff.</description>
        <errorConditionFormula>!ISPICKVAL(Approval_Status__c, &quot;Not Submitted&quot;) &amp;&amp;
IF(
   $User.FirstName &amp; &quot; &quot; &amp; $User.LastName = Approving_Manager__c,
   FALSE,
   IF(
      $User.FirstName &amp; &quot; &quot; &amp; $User.LastName = PTO_Approver__c,
      FALSE,
      IF(
         CONTAINS($Profile.Name, &quot;Admin&quot;),
         FALSE,
         IF(
            CONTAINS($Profile.Name, &quot;Human Resources&quot;),
            FALSE,
            IF(
               $User.User_Full_Name__c = Alternate_PTO_Approver__c, 
               FALSE, 
               TRUE
)))))</errorConditionFormula>
        <errorDisplayField>Approval_Status__c</errorDisplayField>
        <errorMessage>You do not have permission to edit this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>From_Date_and_To_Date_Same_Week_Only</fullName>
        <active>false</active>
        <description>SFDC Support Test Validation Rule</description>
        <errorConditionFormula>Case( 
MOD( DATE(YEAR( DATEVALUE(From_Date__c) ), 1, 1) - DATE (1900, 1, 7), 7), 
0, ( DATEVALUE(From_Date__c) - DATE(YEAR(DATEVALUE(From_Date__c)),1,1) + 3) /7, 
1, ( DATEVALUE(From_Date__c) - DATE(YEAR(DATEVALUE(From_Date__c)),1,1) + 4) /7, 
2, ( DATEVALUE(From_Date__c) - DATE(YEAR(DATEVALUE(From_Date__c)),1,1) + 5) /7, 
3, ( DATEVALUE(From_Date__c) - DATE(YEAR(DATEVALUE(From_Date__c)),1,1) + 6) /7, 
4, ( DATEVALUE(From_Date__c) - DATE(YEAR(DATEVALUE(From_Date__c)),1,1) + 7) /7, 
5, ( DATEVALUE(From_Date__c) - DATE(YEAR(DATEVALUE(From_Date__c)),1,1) + 1) /7, 
6, ( DATEVALUE(From_Date__c) - DATE(YEAR(DATEVALUE(From_Date__c)),1,1) + 2) /7, 
99) 
&lt;&gt; 
Case( 
MOD( DATE(YEAR(DATEVALUE( To_Date__c )), 1, 1) - DATE (1900, 1, 7), 7), 
0, ( DATEVALUE( To_Date__c )- DATE(YEAR(DATEVALUE( To_Date__c )),1,1) + 3) /7, 
1, ( DATEVALUE( To_Date__c )- DATE(YEAR(DATEVALUE( To_Date__c )),1,1) + 4) /7, 
2, ( DATEVALUE( To_Date__c )- DATE(YEAR(DATEVALUE( To_Date__c )),1,1) + 5) /7, 
3, ( DATEVALUE( To_Date__c )- DATE(YEAR(DATEVALUE( To_Date__c )),1,1) + 6) /7, 
4, ( DATEVALUE( To_Date__c )- DATE(YEAR(DATEVALUE( To_Date__c )),1,1) + 7) /7, 
5, ( DATEVALUE( To_Date__c )- DATE(YEAR(DATEVALUE( To_Date__c )),1,1) + 1) /7, 
6, ( DATEVALUE( To_Date__c )- DATE(YEAR(DATEVALUE( To_Date__c )),1,1) + 2) /7, 
99)</errorConditionFormula>
        <errorDisplayField>From_Date__c</errorDisplayField>
        <errorMessage>Please make separate Time Off Requests for time off occurring on different weeks.</errorMessage>
    </validationRules>
