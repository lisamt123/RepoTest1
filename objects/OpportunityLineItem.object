    <fields>
        <fullName>Pricing_HTML__c</fullName>
        <externalId>false</externalId>
        <formula>&apos;&lt;tr&gt;&lt;td&gt;&apos; &amp; PricebookEntry.Product2.Name &amp; &apos;&lt;/td&gt;&lt;td&gt;&apos; &amp; 
IF( ISNULL( PricebookEntry.Product2.Description ) , &apos;&apos;, PricebookEntry.Product2.Description) &amp; 
&apos;&lt;/td&gt;&lt;td&gt;&apos; &amp; TEXT(Quantity) &amp; &apos;&lt;/td&gt;&lt;td&gt;$&apos; &amp; 
IF(TotalPrice/Quantity &gt;= 1000000, TEXT(FLOOR(TotalPrice/Quantity / 1000000)) &amp; &quot;,&quot;, &quot;&quot;) &amp; 
IF(TotalPrice/Quantity &gt;= 1000, RIGHT(TEXT(FLOOR(TotalPrice/Quantity / 1000)), 3) &amp; &quot;,&quot;, &quot;&quot;) &amp; 
RIGHT(TEXT(FLOOR(TotalPrice/Quantity)), 3) &amp; &quot;.&quot; &amp; 
IF(MOD(TotalPrice/Quantity , 1) * 100 &lt; 10, &quot;0&quot; &amp; TEXT(ROUND(MOD(TotalPrice/Quantity , 1), 2) * 100), TEXT(MIN(ROUND(MOD(TotalPrice/Quantity , 1), 2) * 100, 99))) &amp; 
&apos;&lt;/td&gt;&lt;td&gt;$&apos; &amp; 
IF(TotalPrice&gt;= 1000000, TEXT(FLOOR(TotalPrice/ 1000000)) &amp; &quot;,&quot;, &quot;&quot;) &amp; 
IF(TotalPrice&gt;= 1000, RIGHT(TEXT(FLOOR(TotalPrice/ 1000)), 3) &amp; &quot;,&quot;, &quot;&quot;) &amp; 
RIGHT(TEXT(FLOOR(TotalPrice)), 3) &amp; &quot;.&quot; &amp; 
IF(MOD(TotalPrice, 1) * 100 &lt; 10, &quot;0&quot; &amp; TEXT(ROUND(MOD(TotalPrice, 1), 2) * 100), TEXT(MIN(ROUND(MOD(TotalPrice, 1), 2) * 100, 99))) &amp; 
&apos;&lt;/td&gt;&lt;/tr&gt;&apos;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
