    <fields>
        <fullName>TNOPPST__Account_Number__c</fullName>
        <deprecated>false</deprecated>
        <description>Account Number for Avalara</description>
        <externalId>false</externalId>
        <inlineHelpText>Avalara Account Number.  Can also be a Dashboard UserName, but this is not reccomended.  If Supplying a UserName instead of an Account Number, put the password into the License Key field.</inlineHelpText>
        <label>Account Number</label>
        <length>50</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>TNOPPST__Active_Setting__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>If this is checked, this is the TaxNowSetting record that will be referenced for Avalara Credentials, Origin Address and Tax Now Options.  Only one record can have this checked, and checking for this record will uncheck the one currently checked.</inlineHelpText>
        <label>Active TaxNow Setting</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>TNOPPST__Automatic_Tax_Calculation__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>UnChecked by default.  If this Box is checked AND Tax Calculations are Enabled, Tax may be recalculated upon Saving the Quote or Opportunity (recalculation will only be done if a value which might affect the tax calculation has changed)</inlineHelpText>
        <label>Automatic Tax Calculation</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>TNOPPST__Commit_On_Status__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>If Automatic Tax Calculation is Checked, AND Enable Commit is checked, when the status (Quotes) or stage (Opportunities) is set to correspond to this value, the Invoice will be Committed to Avalara upon Save.</inlineHelpText>
        <label>Finalized Status/Stage</label>
        <length>50</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>TNOPPST__Company_Code__c</fullName>
        <deprecated>false</deprecated>
        <description>Company Code for Avalara Tax Calcs</description>
        <externalId>false</externalId>
        <inlineHelpText>Dashboard Company Code that will be referenced in Tax Calculations for this TaxNowSetting Object.</inlineHelpText>
        <label>Company Code</label>
        <length>50</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>TNOPPST__Enable_Address_Validation__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>Enables Address Validation</description>
        <externalId>false</externalId>
        <inlineHelpText>Checked by default.  If unchecked, Address Validations are disabled.</inlineHelpText>
        <label>Enable Address Validation</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>TNOPPST__Enable_Commits__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Enable Committing to Avalara</description>
        <externalId>false</externalId>
        <inlineHelpText>Unchecked by default.  Check this if you wish to be able to Commit Invoices to Avalara.  Note that once an Invoice is committed, it cannot have its details (line amounts, etc,) changed.</inlineHelpText>
        <label>Enable Commits</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>TNOPPST__Enable_Tax_Calculation__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>Enables Tax Calcs for Avalara</description>
        <externalId>false</externalId>
        <inlineHelpText>Checked by default.  If unchecked, Sales Tax Calculation is disabled.</inlineHelpText>
        <label>Enable Tax Calculation</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>TNOPPST__Last_Validated__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Last Validated</label>
        <length>30</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
