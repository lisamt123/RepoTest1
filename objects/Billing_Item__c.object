    <validationRules>
        <fullName>BI_Start_cannot_be_before_Contract_Start</fullName>
        <active>false</active>
        <description>the billing item start date cannot be before the Contract Start Date</description>
        <errorConditionFormula>IF( Start_Date__c &lt;  Contract__r.StartDate , TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>Access_to_GA__c</errorDisplayField>
        <errorMessage>The Product Billing Start Date cannot be before the Contract Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Credit_Amount_must_be_negative_number</fullName>
        <active>true</active>
        <description>when creating a line item for a credit, the amount must be a negative number</description>
        <errorConditionFormula>OR( 
Product_Code__c = &quot;PV Appl Dep&quot;, 
Product_Code__c = &quot;CQ APLD DEPOSIT&quot;, 
Product_Code__c = &quot;PV Promo&quot;, 
Product_Code__c = &quot;GL Promo&quot; 
) 
&amp;&amp; 
RecordType.DeveloperName = &quot;ADD&quot; &amp;&amp; 
Total_Price__c &gt;= 0 &amp;&amp; 
$Setup.Master_Switches__c.Validation_Rules__c = TRUE</errorConditionFormula>
        <errorDisplayField>Sale_Price__c</errorDisplayField>
        <errorMessage>Please add the negative sign in front of the amount and then save</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_cannot_be_less_than_start_date</fullName>
        <active>true</active>
        <description>the end date cannot be less than the start date- if the start date is deferred user must do a modify first for the start date, then do the stop action</description>
        <errorConditionFormula>End_Date__c &lt; Start_Date__c &amp;&amp;
$Setup.Master_Switches__c.Validation_Rules__c = TRUE</errorConditionFormula>
        <errorDisplayField>Access_to_GA__c</errorDisplayField>
        <errorMessage>You must update process a Modify, set the start date to the same as the requested end date, transmit change, then process a stop billing function.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enforce_Stop_Billing_Process</fullName>
        <active>false</active>
        <description>ensure that users are using the stop billing process</description>
        <errorConditionFormula>RecordType.Name = &quot;MODIFY&quot; &amp;&amp; 
ISCHANGED(End_Date__c )&amp;&amp;
!ISPICKVAL( Change_Type__c , &quot;Module Remove&quot;)&amp;&amp;
 Business_Unit__c != &quot;VinSolutions&quot;</errorConditionFormula>
        <errorMessage>you must use the stop billing button on the billed asset to apply an end date and stop billing in RMB</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enforce_Stop_Billing_Process_swap</fullName>
        <active>false</active>
        <description>ensure that users are using the stop billing process</description>
        <errorConditionFormula>RecordType.Name = &quot;MODIFY&quot; &amp;&amp; 
ISCHANGED(End_Date__c )&amp;&amp;
!ISPICKVAL( Change_Type__c , &quot;Swap&quot;)&amp;&amp;
 Business_Unit__c != &quot;VinSolutions&quot;</errorConditionFormula>
        <errorMessage>you must use the stop billing button on the billed asset to apply an end date and stop billing in RMB</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LSE_Date_cannot_be_in_the_future</fullName>
        <active>true</active>
        <errorConditionFormula>Last_SE_Date__c &gt; Today()</errorConditionFormula>
        <errorMessage>you cannot set a solid engagement date in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>One_Time_Recurring_Flag_must_be_null</fullName>
        <active>false</active>
        <description>when billing recurring product as a one time charge, meaning there is a start and end date, ensure the recurring flag is null</description>
        <errorConditionFormula>IF( 
   AND( 
      Product_Family__c = &quot;Monthly Reoccuring&quot;,
      TEXT(Action__c) != &quot;STOP&quot;, 
      TEXT(Action__c) !=&quot;Cancelled&quot;,
      End_Date__c &gt;= Today(), 
      End_Date__c &lt;= Today(),
      ISPICKVAL( Recurring_Flag__c , &quot;BP&quot;)
   ),
   True, 
   False 
)</errorConditionFormula>
        <errorMessage>Please clear the Recurring Flag when billing recurring product as a one time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Price_is_Changed</fullName>
        <active>true</active>
        <description>if the price is changed and the ready to bill box is not checkbox - warn user</description>
        <errorConditionFormula>ISCHANGED(Total_Price__c) &amp;&amp;
Ready_to_Bill__c = False &amp;&amp;
RecordType.DeveloperName != &quot;ADD&quot; &amp;&amp;
$Setup.Master_Switches__c.Validation_Rules__c = TRUE &amp;&amp;
 $User.Alias != &quot;mjoli&quot;</errorConditionFormula>
        <errorMessage>You must submit price changes to RMB. Update the Ready to Bill box on the Billed Asset, and update the Ready to Bill box on the Contract to transmit change to RMB.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Promo_Code_Line_Must_Have_an_End_Date</fullName>
        <active>true</active>
        <description>Cannot save Promo Code Line without an End Date</description>
        <errorConditionFormula>OR( 
Product_Code__c = &quot;PV Promo&quot;, 
Product_Code__c = &quot;GL Promo&quot; 
) 
&amp;&amp; 
RecordType.DeveloperName = &quot;ADD&quot; &amp;&amp; 
  ISBLANK(End_Date__c)
&amp;&amp; 
$Setup.Master_Switches__c.Validation_Rules__c = TRUE</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Please add the Billing End Date for the Promo Code and then save</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Recurring_Flag_Required</fullName>
        <active>true</active>
        <description>if the product is recurring with no end date, the recurring flag is required</description>
        <errorConditionFormula>Product_Family__c = &quot;Monthly Reoccuring&quot; &amp;&amp;
ISBLANK(End_Date__c) &amp;&amp;
ISPICKVAL(Recurring_Flag__c, &quot;NULL&quot;) &amp;&amp;
$Setup.Master_Switches__c.Validation_Rules__c = TRUE</errorConditionFormula>
        <errorMessage>You must set the Recurring Flag to BP when billing recurring product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Related_POA_Required_vauto</fullName>
        <active>false</active>
        <description>user must look up the related POA when adding line items</description>
        <errorConditionFormula>Business_Unit__c = &quot;vAuto&quot; &amp;&amp;
ISBLANK(Related_POA__r.Name) &amp;&amp;
Ready_to_Bill__c = TRUE &amp;&amp;
$Setup.Master_Switches__c.Validation_Rules__c = TRUE</errorConditionFormula>
        <errorMessage>You must assign the POA for this Account, so the new Billed Asset will show in the PM&apos;s list.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_date_is_required_for_RMB</fullName>
        <active>true</active>
        <description>the start date is required before sending a line to RMB</description>
        <errorConditionFormula>Ready_to_Bill__c = True &amp;&amp;  ISNULL( Start_Date__c ) &amp;&amp; 
$Setup.Master_Switches__c.Validation_Rules__c = TRUE</errorConditionFormula>
        <errorMessage>you must provide a start date before sending to RMB</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Use_Alt_Description_Required</fullName>
        <active>false</active>
        <description>if the user types in an alternate decription, ensure the &quot;Use Alternate Description&quot; checkbox is set to true</description>
        <errorConditionFormula>ISBLANK(Alternate_Product_Description__c) &amp;&amp;
Use_Alternate_Description__c = TRUE &amp;&amp;
$Setup.Master_Switches__c.Validation_Rules__c = TRUE</errorConditionFormula>
        <errorMessage>You must check the box &quot;Use Alternate Description&quot; before sending to RMB.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>cannot_change_setup_end_date</fullName>
        <active>true</active>
        <description>you cannot change the account setup end date</description>
        <errorConditionFormula>Product_Sold__r.Name = &quot;Account Setup&quot; &amp;&amp;
