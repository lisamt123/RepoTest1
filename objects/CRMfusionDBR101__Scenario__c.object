        <fullName>CRMfusionDBR101__Custom_Apex_Handler</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR(  TEXT( CRMfusionDBR101__Match_on_Insert_Action__c ) = &apos;Custom Apex Code&apos;, TEXT( CRMfusionDBR101__Match_on_Update_Action__c ) = &apos;Custom Apex Code&apos; ), ISBLANK(  CRMfusionDBR101__Apex_Handler__c ) )</errorConditionFormula>
        <errorDisplayField>CRMfusionDBR101__Apex_Handler__c</errorDisplayField>
        <errorMessage>If the Match on Insert Action or Match on Update Action is set to &quot;Custom Apex Code&quot; then an Apex class must be selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRMfusionDBR101__Do_Not_Insert_Error_Message_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( CRMfusionDBR101__Match_on_Insert_Action__c, &apos;Do Not Insert&apos; ),
OR( ISNULL( CRMfusionDBR101__Error_Message__c  ), LEN(  TRIM( CRMfusionDBR101__Error_Message__c ) )  = 0 ) )</errorConditionFormula>
        <errorDisplayField>CRMfusionDBR101__Error_Message__c</errorDisplayField>
        <errorMessage>If &quot;Match on Insert Action&quot; is set to &quot;Do Not Insert&quot;, you must provide an error message.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRMfusionDBR101__Do_Not_Update_Error_Message_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( CRMfusionDBR101__Match_on_Update_Action__c, &apos;Do Not Update&apos; ),
OR( ISNULL( CRMfusionDBR101__Error_Message__c  ), LEN(  TRIM( CRMfusionDBR101__Error_Message__c ) )  = 0 ) )</errorConditionFormula>
        <errorDisplayField>CRMfusionDBR101__Error_Message__c</errorDisplayField>
        <errorMessage>If &quot;Match on Update Action&quot; is set to &quot;Do Not Update&quot;, you must provide an error message.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRMfusionDBR101__Hash_Key_Bit_Size</fullName>
        <active>true</active>
        <errorConditionFormula>OR( ISNULL( CRMfusionDBR101__Hash_Key_Bits__c ), OR( CRMfusionDBR101__Hash_Key_Bits__c &lt; 8,  CRMfusionDBR101__Hash_Key_Bits__c &gt; 24 ) )</errorConditionFormula>
