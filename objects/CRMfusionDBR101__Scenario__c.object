    <webLinks>
        <fullName>CRMfusionDBR101__Rebuild_Scenario_Keys_Detail</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Rebuild Scenario Keys</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var nameSpace = &apos;CRMfusionDBR101&apos;;
var error = false;

if ( {!CRMfusionDBR101__Scenario__c.CRMfusionDBR101__Scenario_Rule_Count__c} &lt; 1 )
{
    error = true;
    alert( &quot;{!JSINHTMLENCODE($Label.CRMfusionDBR101__DB_Error_Scenario_Needs_Rules)}&quot; );
}

if ( !error &amp;&amp; {!CRMfusionDBR101__Scenario__c.CRMfusionDBR101__Deployed__c} == false )
{
    error = true;
    alert( &quot;{!JSINHTMLENCODE($Label.CRMfusionDBR101__DB_Error_Scenario_Not_Deployed)}&quot; );
}

if ( !error )
{
    window.location.href = &apos;/apex/&apos; + nameSpace + &apos;__DB_Scenario_VF_Key_Builder?redir_url={!URLENCODE(&apos;/&apos; + CRMfusionDBR101__Scenario__c.Id )}&amp;scenarioIds={!CRMfusionDBR101__Scenario__c.Id}&apos;;
}</url>
    </webLinks>
    <webLinks>
        <fullName>CRMfusionDBR101__Rebuild_Scenario_Keys_List</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Rebuild Scenario Keys</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)}

var nameSpace = &apos;CRMfusionDBR101&apos;;
var scenarioIdsArray = {!GETRECORDIDS( $ObjectType.CRMfusionDBR101__Scenario__c )};
var scenarioIds = &apos;&apos;;
var scenarioIdFilter = &apos;&apos;;
var error = false;

sforce.connection.defaultNamespace = nameSpace;

for ( var x = 0; x &lt; scenarioIdsArray.length; x++ )
{
    if ( x == 0 )
    {
		scenarioIds = scenarioIdsArray[x];
		scenarioIdFilter = &quot;&apos;&quot; + scenarioIdsArray[x] + &quot;&apos;&quot;;
    }
    else
    {
		scenarioIds += &quot;,&quot; + scenarioIdsArray[x];
		scenarioIdFilter += &quot;,&apos;&quot; + scenarioIdsArray[x] + &quot;&apos;&quot;;
    }
}

if ( scenarioIdsArray.length &gt; 0 )
{
    var results = sforce.connection.query( &quot;SELECT COUNT() &quot; +
        &quot;FROM Scenario__c &quot; +
        &quot;WHERE Id IN (&quot; + scenarioIdFilter + &quot;) AND Deployed__c = true &quot; +
        &quot;AND Id IN (SELECT Scenario__c FROM Scenario_Rule__c)&quot; );

    if ( results.size &lt; scenarioIdsArray.length )
    {
        error = true;
        alert( &quot;{!JSINHTMLENCODE($Label.CRMfusionDBR101__DB_Error_KB_Some_Scenarios_Not_Deployed)}&quot; );
    }
}
else
{
    // No scenarios were selected, throw an error.
    error = true;
    alert( &quot;{!JSINHTMLENCODE($Label.CRMfusionDBR101__DB_Error_KB_No_Scenarios_Selected)}&quot; );
}

var results = sforce.connection.query( &quot;SELECT COUNT() &quot; +
    &quot;FROM Batch_Key_Builder_Data__c &quot; +
    &quot;WHERE Completed__c = false LIMIT 1&quot; );

if ( results.size &gt; 0 )
{
    error = true;
    alert( &quot;The batch key builder is currently running.  Only one batch key rebuild may be run at one time.  Either cancel the current rebuild or wait until it completes.  To view the status of the rebuild process or cancel it, open a scenario and then click the key rebuild status link in the notice at the top of the page.&quot; );
}

if ( !error )
{
    window.location.href = &apos;/apex/&apos; + nameSpace + &apos;__DB_Scenario_VF_Key_Builder?scenarioIds=&apos; + scenarioIds + &apos;&amp;redir_url={!URLENCODE( URLFOR($Action.CRMfusionDBR101__Scenario__c.List,  $ObjectType.CRMfusionDBR101__Scenario__c ))}&apos; ;
}</url>
    </webLinks>
