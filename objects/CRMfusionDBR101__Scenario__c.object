        <errorConditionFormula>AND( NOT( ISNULL( CRMfusionDBR101__Max_Blocks_Shown__c ) ), OR( CRMfusionDBR101__Max_Blocks_Shown__c &lt; 1, CRMfusionDBR101__Max_Blocks_Shown__c &gt; 20 ) )</errorConditionFormula>
        <errorDisplayField>CRMfusionDBR101__Max_Blocks_Shown__c</errorDisplayField>
        <errorMessage>If a value is set, it must be between 1 and 20.</errorMessage>
    </validationRules>
