    <fields>
        <fullName>Order__c</fullName>
        <externalId>false</externalId>
        <formula>(SLX__Days_Outstanding__c +1 ) * 0.25 *
CASE( SLX__Request_Status__c , &quot;Completed&quot;, 0, &quot;Denied&quot;, 0, &quot;On Hold&quot;, 0, &quot;Cancelled&quot;, 0, &quot;Future&quot;, 0.1,1) *
CASE( Priority__c , &quot;5-Cold&quot;, 1, &quot;4-Lukewarm&quot;, 2, &quot;3-Warm&quot;, 4, &quot;2-Hot&quot;, 8, &quot;1-ON FIRE&quot;, 16, 1) *
CASE( Time_to_Completion__c , &quot;Quick Fix&quot;, 5, &quot;Half Day&quot;, 2, &quot;1 Day&quot;, 1, &quot;2 Days+&quot;, 0.5, 1) *
CASE( Change_Type__c, &quot;Customer-Facing Issue&quot;, 4, &quot;Broken Functionality&quot;, 3, 1) * 
CASE( Department3__c , 
&quot;Executive Team&quot;, 4, 
&quot;Sales&quot;, 3,
&quot;Creative&quot;, 2,
&quot;Fulfillment&quot;, 2,
&quot;Human Resources&quot;, 2,
&quot;Implementation&quot;, 1.5,
&quot;Integration&quot;, 2,
&quot;Process Marketing&quot;, 2,
&quot;VinCare&quot;, 3,
&quot;VinPros&quot;, 3,
&quot;Training&quot;, 2,
&quot;Virtual Training&quot;, 2,
&quot;Website Support&quot;, 1.5,
1) +
IF( Force_to_Top__c = TRUE, 9001, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Order</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
