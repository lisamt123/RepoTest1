    <validationRules>
        <fullName>Cannot_change_Approval_Status</fullName>
        <active>true</active>
        <description>Approval status can not be changed to Approved</description>
        <errorConditionFormula>OR(
   (
      ISNEW() &amp;&amp;
      !ISPICKVAL(Approval_Status__c, &quot;Not Submitted&quot;)
   ),
   (
      PRIORVALUE (Approval_Status__c) = &quot;Not Submitted&quot; &amp;&amp;
      ISPICKVAL(Approval_Status__c, &quot;Approved&quot;)
   )
)
&amp;&amp;
   NOT( 
      OR(
         $User.UserRoleId = &quot;00E70000001GBsB&quot;, /*Senior Business Analyst*/
         $User.UserRoleId = &quot;00E70000001GJu5&quot;, /*CRM Systems Administrator*/
         $User.UserRoleId = &quot;00E70000001GE8B&quot; /*Salesforce Developer*/
      )
   )</errorConditionFormula>
        <errorDisplayField>Approval_Status__c</errorDisplayField>
        <errorMessage>You cannot set the status to &quot;Approved&quot; before going through the Approval process.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Credit_Requested_Check</fullName>
        <active>true</active>
        <description>If credit is requested, there must be an amount added.</description>
        <errorConditionFormula>ISPICKVAL(Credit_Requested__c, &quot;YES&quot;) &amp;&amp; ISBLANK(Amount_Requested_Credit__c)</errorConditionFormula>
        <errorDisplayField>Amount_Requested_Credit__c</errorDisplayField>
        <errorMessage>If credit is requested, then you must enter an amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Effective_Date</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Date_Credited__c ) &amp;&amp;
NOT ( OR(
   $User.UserRoleId = &quot;00E70000001GBsB&quot;, /*Senior Business Analyst*/
   $User.UserRoleId = &quot;00E70000001GJu5&quot;, /*CRM Systems Administrator*/
   $User.UserRoleId = &quot;00E70000001GFEU&quot;, /*Customer Service Representative*/
   $User.UserRoleId = &quot;00E70000001GE8B&quot; /*Salesforce Developer*/
))</errorConditionFormula>
        <errorDisplayField>Date_Credited__c</errorDisplayField>
        <errorMessage>You must enter a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_New_Provider_for_Cancel_Downgrad</fullName>
        <active>true</active>
        <description>Cancellations and Downgrades require that you specify the name of the company we are losing their business to.</description>
        <errorConditionFormula>OR(
   ISPICKVAL(Why_are_we_issuing_credit__c, &quot;Cancellation&quot;),
   ISPICKVAL(Why_are_we_issuing_credit__c, &quot;Downgrade&quot;)
) &amp;&amp;
ISBLANK(New_Provider__c)</errorConditionFormula>
        <errorDisplayField>New_Provider__c</errorDisplayField>
        <errorMessage>Cancellations and Downgrades require that you specify the name of the company we are losing their business to.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Reasons_for_Cancel_Downgrade</fullName>
        <active>true</active>
        <description>Please enter the reason the client is cancelling or downgrading.</description>
        <errorConditionFormula>OR(
   ISPICKVAL(Why_are_we_issuing_credit__c, &quot;Cancellation&quot;),
   ISPICKVAL(Why_are_we_issuing_credit__c, &quot;Downgrade&quot;)
) &amp;&amp;
ISBLANK(TEXT(Primary_Reason_for_Cancellation__c))</errorConditionFormula>
        <errorDisplayField>Primary_Reason_for_Cancellation__c</errorDisplayField>
        <errorMessage>Please enter the reason the client is cancelling or downgrading.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Amount_Approved</fullName>
        <active>true</active>
        <description>Restricts changing the amount approved to certain users.</description>
        <errorConditionFormula>AND(  
      ISCHANGED( Amount_Approved__c ), 
      NOT ( OR( 
            $User.ProfileId = &quot;00e70000001FiMt&quot;, /*VinSet Paid Button Hitter*/
            $User.ProfileId = &quot;00e70000000wzUb&quot;, /*VinExecutive user*/
            $User.ProfileId = &quot;00e70000000wzXQ&quot;, /*VinSystem Admin*/
            $User.ProfileId = &quot;00e70000001FaWM&quot;, /*VinSystem Dev Admin*/
            $User.ProfileId = &quot;00e70000001FiDh&quot; /*VinSystem Jr Admin*/
      ) ) 
   )</errorConditionFormula>
        <errorDisplayField>Amount_Approved__c</errorDisplayField>
        <errorMessage>You do not have permission to change this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Approval_Status_Change</fullName>
        <active>true</active>
        <description>Restricts changing the approval status dropdown to certain users.</description>
        <errorConditionFormula>AND(  
      ISCHANGED( Approval_Status__c ), 
      NOT ( OR( 
            $User.ProfileId = &quot;00e70000001FiMt&quot;, /*VinSet Paid Button Hitter*/
            $User.ProfileId = &quot;00e70000000wzUb&quot;, /*VinExecutive user*/
            $User.ProfileId = &quot;00e70000000wzXQ&quot;, /*VinSystem Admin*/
            $User.ProfileId = &quot;00e70000001FaWM&quot;, /*VinSystem Dev Admin*/
            $User.ProfileId = &quot;00e70000001FiDh&quot; /*VinSystem Jr Admin*/
      ) ) 
   )</errorConditionFormula>
        <errorDisplayField>Approval_Status__c</errorDisplayField>
        <errorMessage>You do not have permission to change this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Training_Requested_Check</fullName>
        <active>true</active>
        <description>If free training is requested, an amount must be entered.</description>
        <errorConditionFormula>ISPICKVAL(Free_Training_Requested__c, &quot;YES&quot;) &amp;&amp; ISBLANK( Amount_of_Training_Days_Requested__c )</errorConditionFormula>
        <errorDisplayField>Amount_of_Training_Days_Requested__c</errorDisplayField>
        <errorMessage>If free training is requested, you must enter an amount.</errorMessage>
    </validationRules>
