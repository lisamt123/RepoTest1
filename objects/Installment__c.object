    <validationRules>
        <fullName>Already_marked_confirmed</fullName>
        <active>false</active>
        <description>Checks to see if the opportunity was already marked confirmed</description>
        <errorConditionFormula>ISPICKVAL(Opportunity__r.Confirmed__c, &quot;Yes&quot;) &amp;&amp; 
Installment_Number__c ==1 &amp;&amp; 
Confirmed__c = TRUE &amp;&amp; 
ISCHANGED(Confirmed__c) &amp;&amp;
Opportunity__r.RecordTypeId != &quot;01270000000Q9Me&quot; &amp;&amp;
NOT ( OR(
   $User.UserRoleId = &quot;00E70000001GBsB&quot;, /*Senior Business Analyst*/
   $User.UserRoleId = &quot;00E70000001GJu5&quot;, /*CRM Systems Administrator*/
   $User.UserRoleId = &quot;00E70000001GFEU&quot;, /*Customer Service Representative*/
   $User.UserRoleId = &quot;00E70000001GE8B&quot;, /*Salesforce Developer*/
   $User.UserRoleId = &quot;00E70000001G9uk&quot; /*Program Manager (Ryan)*/
) )</errorConditionFormula>
        <errorMessage>First installment has already been confirmed and the project generated.  Please see an administrator if you have any questions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Confirm_Old_Opps</fullName>
        <active>false</active>
        <errorConditionFormula>ISCHANGED(Confirmed__c) &amp;&amp;
Confirmed__c = TRUE &amp;&amp;
Paid__c &lt; DATETIMEVALUE(DATE(2013,6,1))</errorConditionFormula>
        <errorMessage>You cannot confirm an opp this old.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Confirm_if_No_Website</fullName>
        <active>true</active>
        <description>when a Haystak opp is closed won, the website url required</description>
        <errorConditionFormula>Opportunity__r.IsWon &amp;&amp;
ISBLANK(Opportunity__r.Website__c) &amp;&amp;
Opportunity__r.RecordType.DeveloperName = &quot;Digital_Marketing&quot; &amp;&amp;
OR(
   ISCHANGED(Confirmed__c) &amp;&amp; Confirmed__c = TRUE,
   ISCHANGED(Paid__c) &amp;&amp; !ISBLANK(Paid__c)
) &amp;&amp;
ValidationRules__c = FALSE</errorConditionFormula>
        <errorMessage>You must enter the website url for this product request into the opp before you can confirm.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invoice_not_generated</fullName>
        <active>false</active>
        <description>Checks to see if invoice has been generated</description>
        <errorConditionFormula>if( Opportunity__r.Invoice_Generated__c ==true &amp;&amp; Installment_Number__c ==1 &amp;&amp; text(Paid__c)!=&quot;&quot;,TRUE, FALSE)</errorConditionFormula>
        <errorMessage>Invoice needs to be generated before the opportunity can be marked paid.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Paid_Field_Restrictions</fullName>
        <active>false</active>
        <description>Must have the VinSystem Admin, Controller, VP of Sales, Director of Inside Sales, or Director of Sales role to change the Paid field.</description>
        <errorConditionFormula>IF(   
   AND( 
      ISCHANGED( Paid__c ),
      NOT ( OR(
         $User.UserRoleId = &quot;00E70000001GBsB&quot;, /*Business Analyst*/
         $User.UserRoleId = &quot;00E70000000xjyA&quot;, /*Controller*/
         $User.UserRoleId = &quot;00E70000000wHfS&quot;, /*VP of Sales*/
         $User.UserRoleId = &quot;00E70000001GJrp&quot;, /*Director of Inside Sales*/
         $User.UserRoleId = &quot;00E70000001GJu5&quot;, /*CRM Systems Administrator*/
         $User.UserRoleId = &quot;00E70000001GFEU&quot;, /*Customer Service Representative*/
         $User.UserRoleId = &quot;00E70000001GE8B&quot;, /*Salesforce Developer*/
         $User.UserRoleId = &quot;00E70000000yjXQ&quot;, /*Director of Sales*/
         $User.UserRoleId = &quot;00E70000001G9uk&quot; /*Marketing Manager*/
      ) )
   ),
TRUE,
FALSE
)</errorConditionFormula>
        <errorDisplayField>Paid__c</errorDisplayField>
        <errorMessage>Only the Controller, VP of Sales, Director of Sales, Director of Inside Sales, or Salesforce Admins may change this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Installment_Due_Date</fullName>
        <active>false</active>
        <description>Prevents the Installment Due Date field from being changed if it is not a brand new record.</description>
        <errorConditionFormula>AND (
   ISCHANGED( Installment_Due_Date__c ),
   NOT( ISNEW() ) 
)</errorConditionFormula>
        <errorDisplayField>Installment_Due_Date__c</errorDisplayField>
        <errorMessage>You cannot change the Installment Due Date. Please submit a Salesforce Change Request for this kind of change.</errorMessage>
    </validationRules>
</CustomObject>
