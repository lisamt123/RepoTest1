    <validationRules>
        <fullName>CRMfusionDBR101__Contact_Owner_Null_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND( TEXT( CRMfusionDBR101__Scenario__r.CRMfusionDBR101__Scenario_Type__c ) = &apos;Lead to Account&apos;, OR( ISNULL( TEXT( CRMfusionDBR101__Contact_Owner__c ) ), TEXT( CRMfusionDBR101__Contact_Owner__c ) = &apos;&apos; ) )</errorConditionFormula>
        <errorDisplayField>CRMfusionDBR101__Contact_Owner__c</errorDisplayField>
        <errorMessage>The Contact Owner must be set for Lead to Account Auto-Convert scenarios.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRMfusionDBR101__Conversion_Target_Not_Null</fullName>
        <active>true</active>
        <errorConditionFormula>OR( ISNULL( TEXT( CRMfusionDBR101__Conversion_Target__c ) ), TEXT( CRMfusionDBR101__Conversion_Target__c ) = &apos;&apos; )</errorConditionFormula>
