var checked= {!GETRECORDIDS($ObjectType.Change_Comment__c)}; //grabs the records that the user is requesting to update
var record = (checked.toString()); //converts record variable to text string

window.open(&apos;/email/author/emailauthor.jsp?retURL=/{!SLX__Change_Control__c.Id}&amp;p24={!SLX__Change_Control__c.Requestor_s_Email__c}&amp;p4={!SLX__Change_Control__c.Reports_To_Email__c}&amp;p3_lkid=&apos; + record + &apos;&amp;template_id=00X70000001SBNg&apos;);

var markSent = new sforce.SObject(&quot;Change_Comment__c&quot;);
markSent.Id = checked;
markSent.Sent__c = true;
var result = sforce.connection.update([markSent]);
window.location.href=window.location.href;</url>
