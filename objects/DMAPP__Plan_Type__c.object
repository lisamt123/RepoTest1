    <validationRules>
        <fullName>DMAPP__CurrentOpportunitiesValidRange</fullName>
        <active>true</active>
        <errorConditionFormula>(DMAPP__Current_Ops_Ratio_Green__c &lt; 0 || DMAPP__Current_Ops_Ratio_Green__c &gt; 50) || (DMAPP__Current_Ops_Ratio_Amber__c &lt; 0 || DMAPP__Current_Ops_Ratio_Amber__c &gt; 50) || (DMAPP__Current_Ops_Ratio_Amber__c &gt; DMAPP__Current_Ops_Ratio_Green__c)</errorConditionFormula>
        <errorMessage>Current Opportunities Green and Amber cannot be less than 0% or greater than 5000% and Current Opportunities Green cannot be less than Current Opportunities Amber</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DMAPP__MutualValueValidRange</fullName>
        <active>true</active>
        <errorConditionFormula>(DMAPP__Segmentation_Score_Green__c  &lt; 0 ||  DMAPP__Segmentation_Score_Green__c &gt; 1) ||
(DMAPP__Segmentation_Score_Amber__c &lt; 0 || DMAPP__Segmentation_Score_Amber__c &gt; 1) ||
(DMAPP__Segmentation_Score_Amber__c  &gt;  DMAPP__Segmentation_Score_Green__c)</errorConditionFormula>
        <errorMessage>Mutual Value Green and Amber cannot be less than 0% or greater than 100% and Mutual Value Green cannot be less than Mutual Value Amber</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DMAPP__Plan_Type_Name_Maximum_Length_is_40_char</fullName>
        <active>true</active>
        <description>Plan Type Name Maximum Length is 40 chars</description>
        <errorConditionFormula>LEN(Name) &gt; 40</errorConditionFormula>
        <errorMessage>Plan Type Name Maximum Length is 40 chars</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DMAPP__PotentialOpportunitiesValidRange</fullName>
        <active>true</active>
        <errorConditionFormula>(DMAPP__Potential_Ops_Ratio_Green__c &lt; 0  || DMAPP__Potential_Ops_Ratio_Green__c &gt; 50) || (DMAPP__Potential_Ops_Ratio_Amber__c &lt; 0 || DMAPP__Potential_Ops_Ratio_Amber__c &gt; 50) || (
DMAPP__Potential_Ops_Ratio_Amber__c &gt; DMAPP__Potential_Ops_Ratio_Green__c)</errorConditionFormula>
        <errorMessage>Potential Opportunities Green and Amber cannot be less than 0% or greater than 5000% and Potential Opportunities Green cannot be less than Potential Opportunities Amber</errorMessage>
    </validationRules>
    <validationRules>
