    <validationRules>
        <fullName>Cannot_Complete_without_Milestones_ACC</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(POA_Piece_Stage__c, &quot;Completed&quot;) &amp;&amp;
RecordType.Name = &quot;Accounting&quot; &amp;&amp; 
OR(
   ISBLANK(Third_Parties_Contacted__c),
   ISBLANK(Billing_Line_Items_Removed__c),
   ISBLANK(Canceled_Products__c),
   ISBLANK(Last_Date_of_Service__c)
)</errorConditionFormula>
        <errorMessage>You must fill in all milestones prior to Completing this piece. Canceled Products and Last Date of Service are also required.</errorMessage>
    </validationRules>
