    <validationRules>
        <fullName>DMAPP__Closure_Duration_Check</fullName>
        <active>true</active>
        <errorConditionFormula>DMAPP__Closure_Duration__c &lt; 0</errorConditionFormula>
        <errorDisplayField>DMAPP__Closure_Duration__c</errorDisplayField>
        <errorMessage>Closure duration must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DMAPP__Closure_duration_length</fullName>
        <active>true</active>
        <errorConditionFormula>DMAPP__Closure_Duration__c  &gt; 1000</errorConditionFormula>
        <errorDisplayField>DMAPP__Closure_Duration__c</errorDisplayField>
        <errorMessage>Closure Duration cannot be greater than 1000 days</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DMAPP__Percentage_check_FSP</fullName>
        <active>true</active>
        <errorConditionFormula>DMAPP__Final_Stage_Probability__c &gt; 1 ||  DMAPP__Final_Stage_Probability__c &lt; 0</errorConditionFormula>
        <errorDisplayField>DMAPP__Final_Stage_Probability__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DMAPP__Percentage_check_PP</fullName>
        <active>true</active>
        <errorConditionFormula>DMAPP__Pebble_Percentage__c  &gt; 1 || DMAPP__Pebble_Percentage__c  &lt; 0</errorConditionFormula>
        <errorDisplayField>DMAPP__Pebble_Percentage__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DMAPP__Percentage_check_PT</fullName>
        <active>true</active>
        <errorConditionFormula>DMAPP__Threshold__c  &gt; 1 || DMAPP__Threshold__c &lt; 0</errorConditionFormula>
        <errorDisplayField>DMAPP__Threshold__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DMAPP__Percentage_check_RP</fullName>
        <active>true</active>
        <errorConditionFormula>DMAPP__Rock_Percentage__c &gt; 1 || DMAPP__Rock_Percentage__c &lt; 0</errorConditionFormula>
        <errorDisplayField>DMAPP__Rock_Percentage__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
